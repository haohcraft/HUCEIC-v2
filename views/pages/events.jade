extends ../layout

block stylesheet
	link(rel='stylesheet', href='/ext/datetimepicker/jquery.datetimepicker.css')
	link(rel='stylesheet', href='/ext/jquery.ui/themes/base/jquery.ui.all.css')
block script
	script(type="text/javascript" src='http://maps.google.com/maps/api/js?sensor=false&libraries=places')



block content
	// Includes mixins
	include ../mixins/forms
	include ../mixins/event

	section#content.clearfix
		div.container

			header.page-header.clearfix
				h1.page-title Events

			section#main
				div#inner-content.wrap.clearfix
					div#main.sixteencol.clearfix
						div#events.inner-main
							+event(admin)
			aside#sidebar
				div.sidebar-mailing
					h6.section-title Mailing List
					+forms("mailinglist-vertical")
